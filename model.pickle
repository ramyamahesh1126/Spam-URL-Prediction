      "source": [
        "# Use the forest's predict method on the test data\n",
        "predictions = rf.predict(test_dataset)\n",
        "# Calculate the absolute errors\n",
        "errors = abs(predictions - test_labels)\n",
        "# Print out the mean absolute error (mae)\n",
